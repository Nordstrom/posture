{
  "name": "posture",
  "version": "v0.3.1",
  "description": "an opinionated linter for swagger",
  "main": "index.js",
  "author": "Patrick Dent",
  "license": "Apache-2.0",
  "jest": {
    "collectCoverageFrom": [
      "src/*",
      "!src/warnings.js",
      "!src/errors.js",
      "!src/optimizations.js",
      "!src/cli.js"
    ],
    "rootDir": "src"
  },
  "scripts": {
    "build": "rm -rf ./lib && standard --fix && babel -d ./lib ./src -s",
    "build-fast": "babel -d ./lib ./src -s",
    "test": "standard --fix && jest --coverage",
    "test-watch": "jest --watch",
    "cov": "standard --fix && jest --coverage && open coverage/lcov-report/index.html",
    "try": "rm -rf ./lib && babel -d ./lib ./src -s && node lib/cli.js"
  },
  "repository": "Nordstrom/posture",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babili": "^0.1.4",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "jju": "^1.3.0",
    "lodash": "^4.17.4"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect"
    ]
  },
  "bin": {
    "posture": "./lib/cli.js"
  }
}
